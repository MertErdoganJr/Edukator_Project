@model List<Edukator.EntityLayer.Concreate.Reference>

<section class="w3l-testimonials" id="testimonials">
    <div class="cusrtomer-layout py-5">
        <div class="container py-lg-4 py-md-3 py-2 pb-lg-0">
            <h5 class="title-subw3hny text-center mb-1">Gözden Geçir</h5>
            <h3 class="title-w3l two text-center mb-sm-5 mb-4">
                Müşteriler & <span class="inn-text">Yorumlar</span>
            </h3>
            <div class="testimonial-width">
                <div class="owl-two row">
                    @foreach (var item in Model)
                    {
                        <div class="col-lg-4 col-md-6 item mt-md-5 mt-4">
                            <div class="testimonial-content">
                                <div class="testimonial">
                                    <i class="fas fa-quote-right"></i>
                                    <blockquote>
                                        <q>
                                           @item.Comment
                                        </q>
                                    </blockquote>
                                    <div class="testi-des">
                                        <div class="peopl align-self">
                                            <h3>@item.NameSurname</h3>
                                            <p class="indentity">@item.Job</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>